  Journal-iso              = {Lab on a Chip},
  Keywords-plus            = {ULTRASONIC STANDING-WAVE; OPTICAL TWEEZERS; MANIPULATION; PARTICLES; CHIP; RESONANCES; BIOASSAYS; CHANNELS; TRAPS; FIELD},
