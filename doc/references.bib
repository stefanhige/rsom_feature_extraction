  Journal-iso              = {{J. Fluid Mech.}},
  Keywords                 = {{acoustics; particle/fluid flows; Stokesian dynamics}},
  Keywords-plus            = {{SPHERE; WAVES; DROP; LEVITATION; FIELD; SHAPE}},
  Language                 = {{English}},
